# 服务器配置
PORT=3001
NODE_ENV=development

# JWT 配置
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=7d

# 数据库配置 (腾讯云 TencentDB MySQL)
DB_HOST=your_db_host
DB_PORT=3306
DB_USERNAME=your_db_username
DB_PASSWORD=your_db_password
DB_DATABASE=heiha

# 腾讯云 COS 配置
TENCENT_SECRET_ID=your_secret_id
TENCENT_SECRET_KEY=your_secret_key
COS_BUCKET=your_bucket_name
COS_REGION=ap-guangzhou
COS_CDN_DOMAIN=https://cdn.yourdomain.com

# 腾讯云 VOD 配置
VOD_APP_ID=your_vod_app_id
VOD_PROCEDURE_NAME=your_procedure_name

# 腾讯云人脸识别配置
FACE_REGION=ap-guangzhou

# 微信登录配置
WECHAT_APP_ID=your_wechat_app_id
WECHAT_APP_SECRET=your_wechat_app_secret

# 微信支付配置
WECHAT_PAY_MCH_ID=your_mch_id
WECHAT_PAY_API_KEY=your_api_key
WECHAT_PAY_NOTIFY_URL=https://api.yourdomain.com/api/payment/wechat/notify

# 支付宝配置
ALIPAY_APP_ID=your_alipay_app_id
ALIPAY_PRIVATE_KEY=your_private_key
ALIPAY_PUBLIC_KEY=your_public_key
ALIPAY_NOTIFY_URL=https://api.yourdomain.com/api/payment/alipay/notify

# 新用户注册奖励积分
NEW_USER_BONUS_POINTS=1000

# 每日登录奖励积分
DAILY_LOGIN_POINTS=100
